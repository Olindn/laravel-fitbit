{
    "name": "namelivia/laravel-fitbit",
    "description": "A Fitbit web api library for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "fitbit"],
    "authors": [
        {
            "name": "Jos√© Ignacio Amelivia Santiago",
            "email": "jignacio.amelivia@gmail.com",
            "homepage": "https://namelivia.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*|6.0.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.0.*",
        "graham-campbell/manager": "^3.0|^4.0",
        "namelivia/fitbit-http-php": "dev-feature/initialization_simplification"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^1.1",
        "graham-campbell/testbench": "^4.0|^5.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|3.9.*|4.0.*",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Namelivia\\Fitbit\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Namelivia\\Fitbit\\Tests\\Laravel\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Namelivia\\Fitbit\\Laravel\\FitbitServiceProvider"
            ],
            "aliases": {
                "Fitbit": "Namelivia\\Fitbit\\Laravel\\Facades\\Fitbit"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
